{
  "name": "lzycrazy-brand",
  "version": "1.0.0",
  "scripts": {
    "admin": "npm run dev --prefix admin",
    "frontend": "npm run dev --prefix frontend",
    "backend": "npm run dev --prefix backend",
    "dev": "concurrently \"npm run frontend\" \"npm run admin\" \"npm run backend\"",
    
    "build-admin": "npm run build --prefix admin",
    "build-frontend": "npm run build --prefix frontend",
    "build-backend": "npm run build --prefix backend",
    "build": "concurrently \"npm run build-frontend\" \"npm run build-admin\" \"npm run build-backend\""
  },
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
